@model RolesDeUsuario.Models.Director
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "Layout";
}

<form action="~/Home/LoginD" method="post" class="form-boxG">
   
    <h1>Iniciar sesion</h1>
    <input type="text" asp-for="Ncontrol" required placeholder="Numero de control" />
    <input type="password" asp-for="Contrasena" required placeholder="Contraseña"/>
    <input type="submit" value="Iniciar Sesion" />
    @Html.ValidationSummary()
</form>